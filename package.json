{
  "name": "ninsho-example",
  "version": "0.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src/**/*.ts",
    "dev": "ts-node-dev --respawn src/index.ts dev",
    "test": "jest --config ./jest.config.json --runInBand --coverage",
    "pretry": "docker-compose -f docker-pg.yml up -d --build",
    "try": "tsc && node dist/index.js",
    "posttry": "docker-compose -f docker-pg.yml down"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "ninsho": "^0.0.14",
    "ninsho-hook-account-lock": "^0.0.1",
    "ninsho-module-mailer": "^0.0.5",
    "ninsho-module-pg": "^0.0.8",
    "ninsho-module-secure": "^0.0.13",
    "ninsho-plugin-essential-api": "^0.0.2",
    "ninsho-plugin-immediately-api": "^0.0.2",
    "ninsho-plugin-standard-2fa-api": "^0.0.1",
    "nodemailer": "^6.9.1"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/nodemailer": "^6.4.7",
    "@typescript-eslint/eslint-plugin": "3.0.0",
    "@typescript-eslint/parser": "3.0.0",
    "eslint": "7.32.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}
